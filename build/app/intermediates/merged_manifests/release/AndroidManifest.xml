                    android:host="link-accounts"
                    android:path="/com.gds.customer/success"
                    android:scheme="stripe-auth" />
                <data
                    android:host="link-accounts"
                    android:path="/com.gds.customer/cancel"
                    android:scheme="stripe-auth" />

